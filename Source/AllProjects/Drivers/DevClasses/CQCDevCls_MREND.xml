    <CQCDC:GenInfo  CQCDC:Base="CQCDevice" CQCDC:Class="MediaRendeer"
                    CQCDC:Prefix="MREND" CQCDC:MUnit="No"
                    CQCDC:Descr="Media Renderering"
                    CQCDC:NameType="Fixed"
                    CQCDC:Power="Yes"/>

    <CQCDC:Fields>

            <CQCDC:Field CQCDC:Name="ActiveRepository">
                <CQCDC:FieldCfg CQCDC:Type="String" CQCDC:Access="ReadOnly"
                                CQCDC:SemType="MediaRepoDrv"/>
            </CQCDC:Field>

            <CQCDC:Field CQCDC:Name="ClearPlaylist">
                <CQCDC:FieldCfg CQCDC:Type="Boolean"
