                <CQCDC:FieldCfg CQCDC:Type="String"
                                CQCDC:Access="ReadAndWrite"
                                CQCDC:LimitType="Enum" CQCDC:LimitOpt="No"/>
            </CQCDC:Field>

    </CQCDC:Fields>

</CQCDC:DevClass>



