<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE CQCDC:DevClass PUBLIC "urn:charmedquark.com:CQC-DeviceClass.DTD" "CQCDeviceClass.DTD">

<CQCDC:DevClass CQCDC:Version="1">

    <!-- General information -->
    <CQCDC:GenInfo  CQCDC:Base="CQCDevice" CQCDC:Class="AVProcessor"
                    CQCDC:Prefix="AVPRC" CQCDC:MUnit="Yes"
                    CQCDC:Descr="A/V Processor"
                    CQCDC:NameType="Fixed"
                    CQCDC:Power="Yes"/>

    <CQCDC:Fields>

            <CQCDC:Field CQCDC:Name="CurAudioMode">
                <CQCDC:FieldCfg CQCDC:Type="String"
                                CQCDC:Access="ReadOnly"
                                CQCDC:LimitType="Enum" CQCDC:LimitOpt="Yes"/>
            </CQCDC:Field>


            <CQCDC:Field CQCDC:Name="SetAudioMode">
                <CQCDC:FieldCfg CQCDC:Type="String"
                                CQCDC:Access="ReadAndWrite" CQCDC:LimitType="Enum"/>
            </CQCDC:Field>

    </CQCDC:Fields>

</CQCDC:DevClass>

