; ----------------------------------------------------------------------------
;  The core toolkit facility. This is used by basically everything else here, at
;  least indirectly and usually directly.
; ----------------------------------------------------------------------------
PROJECT=CQCKit

    SETTINGS
        DIRECTORY   = CQCKit
        DISPLAY     = N/A
        EXPORT      = CQCKITEXPORT
        MSGFILE     = Yes
        TYPE        = SharedLib
        VERSIONED   = Yes
    END SETTINGS

    IDLFILES
        IDLFILE
            SRCFILE = CQCKit_AppCtrl
            GEN     = CLIENT
            NAMEEXT = AppCtrl
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_ClSrv
            GEN     = CLIENT
            NAMEEXT = ClSrv
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_Shared
            GEN     = GLOBALS
            NAMEEXT = Shared
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_Inst
            GEN     = CLIENT
            NAMEEXT = Inst
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_IVCtrl
            GEN     = CLIENT
            NAMEEXT = IVCtrl
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_CQCSrvAdmin
            GEN     = CLIENT
            NAMEEXT = CQCSrvAdmin
        END IDLFILE

        IDLFILE
            SRCFILE = CQCKit_Security
            GEN     = CLIENT
            NAMEEXT = Security
        END IDLFILE

    END IDLFILES

    EXTLIBS
        CIDMData$(CIDLibVer).Lib
        CIDMath$(CIDLibVer).Lib
        CIDEncode$(CIDLibVer).Lib
        CIDMacroEng$(CIDLibVer).Lib
        CIDSock$(CIDLibVer).Lib
        CIDOrb$(CIDLibVer).Lib
        CIDOrbUC$(CIDLibVer).Lib
        CIDComm$(CIDLibVer).Lib
        CIDImage$(CIDLibVer).Lib
        CIDImgFact$(CIDLibVer).Lib
        CIDPNG$(CIDLibVer).Lib
        CIDCrypto$(CIDLibVer).Lib
        CIDObjStore$(CIDLibVer).Lib
        CIDRegX$(CIDLibVer).Lib
        CIDXML$(CIDLibVer).Lib
        CIDZLib$(CIDLibVer).Lib
        CIDNet$(CIDLibVer).Lib
    END EXTLIBS

    ; We need to get to some otherwise private include files
    INCLUDEPATHS
        $(CIDLIB_RESDIR)\PrivInclude
    END INCLUDEPATHS

END PROJECT



