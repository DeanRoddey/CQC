; ----------------------------------------------------------------------------
;   The core back end servers that make up the core of CQC, on top of the name server
;   and log server provided at the CIDLib level.
; ----------------------------------------------------------------------------
PROJECT=CQCDataSrv

    SETTINGS
        DIRECTORY   = CQCDataSrv
        DISPLAY     = Console
        MSGFILE     = Yes
    END SETTINGS

    ;
    ; We implement multiple interfaces, for different types of data. This
    ; will let us move them around later if needed, without changing any
    ; of the interfaces.
    ;
    ; The data server access one is the big one, which provides access to all
    ; of the user customization stuff (global actions, events, templates, images
    ; and so forth.)
    ;
    ; The installation server interface is for things like drivers and special
    ; files that aren't commonly accessed.
    ;
    IDLFILES
         IDLFILE
             SRCFILE = ..\RemBrws\CQCRemBrws\CQCRemBrws_DataSrvAcc
             GEN     = SERVER
             NAMEEXT = FileAcc
         END IDLFILE

        IDLFILE
            SRCFILE = ..\CQCKit\CQCKit_Inst
            GEN     = SERVER
            NAMEEXT = Inst
        END IDLFILE

        IDLFILE
            SRCFILE = ..\CQCKit\CQCKit_Security
            GEN     = SERVER
            NAMEEXT = Security
        END IDLFILE

        IDLFILE
            SRCFILE = ..\CQCSysCfg\CQCSysCfg_DataIntf
            GEN     = SERVER
            NAMEEXT = SysCfg
        END IDLFILE

        IDLFILE
            SRCFILE = ..\CoreTech\CQCIR\CQCIR_Repo
            GEN     = SERVER
            NAMEEXT = IR
        END IDLFILE

    END IDLFILES

    DEPENDENTS
        CQCKit
        CQCIR
        CQCRemBrws
        CQCSysCfg
        CQCSrvFW
        GC100Ser
        JAPwrSer
    END DEPENDENTS

    EXTLIBS
        CIDMath$(CIDLibVer).Lib
        CIDRegX$(CIDLibVer).Lib
        CIDOrb$(CIDLibVer).Lib
        CIDOrbUC$(CIDLibVer).Lib
        CIDCrypto$(CIDLibVer).Lib
        CIDSock$(CIDLibVer).Lib
        CIDXML$(CIDLibVer).Lib
        CIDMData$(CIDLibVer).Lib
        CIDZLib$(CIDLibVer).Lib
        CIDObjStore$(CIDLibVer).Lib
    END EXTLIBS

END PROJECT


